// SAI-MAI Data Fragment: Legal KYC & DID-Credentials
fragment LegalCredentials {
  field kyc_hash: String;
  field did_hash: String;
  field compliance_log: String;
  field action_name: String;
  field txid: String;
  field timestamp: ISODate;
  require valid(KYC.credential) && valid(DID.credential);
  require international, federal, and local regulation compliance;
  exportable: true;
  privacy: encrypted_onchain;
  audit_log_linked: true;
}
